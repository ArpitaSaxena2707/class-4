<script>var listUrl = [];
var showInfinite = true;
var gatag = [];
var urlCount = 0;
var storyUuid = "0RwQekBKDU2DA0i6uZfoLK";
function ScrollMe(c) {
var d = c.replace("story_", "").trim();
d = d + "_story";
var a = c.trim();
$("#ulInfinite").each(function() {
var e = "";
$(this).find("li").each(function(g, h) {
var f = $(h).attr("id");
if (f.trim() == a.trim()) {
$(h).addClass("active")
} else {
$(h).removeClass("active")
}
})
});
var b = document.getElementById(d);
b.scrollIntoView();
b.scrollIntoView({
behavior: "auto",
block: "start",
inline: "nearest"
});
$("html, body").animate({
scrollTop: $("#" + a).offset().top - 800
}, 800, "swing")
}
//getLatestNews("");
$(document).ready(function() {
$(".respVideo").find('.affiliation').each(function() {
var section = "tech";
function getCustomAds() {
$("[id^=amazon_]").each(function() {
var divID = $(this).attr('id');
var productID = $(this).text();
if (productID.includes(',')) {
getProductByID(divID, productID, true);
} else {
getProductByID(divID, productID, false);
}
});
}
function getProductByID(id, productID, isDouble) {
var customHTMl = "";
$.ajax({
url: "https://www.hindustantimes.com/fetch-products/?item-ids=" + productID + "&provider=aws-ads-provider&properties=HT," + section,
dataType: 'json',
success: function(response) {
if (isDouble) {
customHTMl += "<div class='cmpgn-cntnr clearfix'>";
}
$.each(response.items, function(index, item) {
if (isDouble) {
customHTMl += "<div class='wd-50'>";
customHTMl += "<div class='box-shdw cmpgn-col'>";
} else {
customHTMl += "<div class='cmpgn-cntnr'>";
customHTMl += "<div class='wd-100 box-shdw cmpgn-col'>";
}
customHTMl += "<div class='cmpng-img cmpngflt'><img data-src='" + item.imageUrls[1] + "' src='/img/zero.gif' class='lazyload'>";
customHTMl += "</div>";
customHTMl += "<div class='cmpgn-prdctdetail'>";
customHTMl += "<p>" + item.title + "</p>";
if (isDouble) {
customHTMl += "<span class='totl-price'>&#8377;" + item.buyingPrice;
if (item.originalPrice != null) {
customHTMl += "<del>&#8377;" + item.originalPrice + "</del>";
}
customHTMl += "</span>";
} else {
customHTMl += "<div class='xs-btn'>";
if (item.buyingPrice != null) {
customHTMl += "<small class='prictag'>&#8377;" + item.buyingPrice + "</small>";
}
customHTMl += "<span class='disnt-price'>";
if (item.originalPrice != null) {
customHTMl += "<del>&#8377;" + item.originalPrice + "</del>";
}
if (item.savingPercentage > 0) {
customHTMl += "<small>(" + item.savingPercentage + "% 0ff)</small>";
}
customHTMl += "</span></div>";
}
customHTMl += "<a href='" + item.detailPageURL + "' class='cmpngbtn' target='_blank'>Buy on Amazon</a>";
customHTMl += "</div>";
if (isDouble) {
customHTMl += "</div>";
customHTMl += "</div>";
} else {
customHTMl += "</div>";
customHTMl += "</div>";
}
});
if (isDouble) {
customHTMl += "</div>";
}
$("#" + id).show();
$("#" + id).html(customHTMl);
}
});
}
getCustomAds();
});
});
function InfiniteScroll() {
var a = listUrl[urlCount];
var d = $(".articles").infiniteScroll({
path: function() {
return a
},
append: ".article",
status: ".scroller-status",
hideNav: ".pagination",
loadOnScroll: false,
scrollThreshold: false
});
d.infiniteScroll("loadNextPage");
d.on("history.infiniteScroll", function(g, i, h) {
var f = "article_" + getStoryIdByUrl(h);
document.title = i;
var e = h.replace(".html", "").split("-");
e = e.reverse();
var k = e[0].trim();
$("#ulInfinite li").removeClass("active");
$("#story_" + k).addClass("active");
ga('send', {
'hitType': 'pageview',
'location': window.location.hostname.trim(),
'page': window.location.pathname.trim(),
'title': i.trim()
});
window.snowplow("trackPageView", i.trim());
if (typeof COMSCORE != 'undefined' && typeof COMSCORE.beacon !== 'undefined'){
COMSCORE.beacon({c1 : "2",c2 : "6035286"})
}
});
d.on("load.infiniteScroll", function(f, e, g) {
urlCount++
});
var b = 1;
var c;
d.on("append.infiniteScroll", function(F, g, A, y) {
