<script>
var preViewportDistance = 1.5 * $(window).height();
/** Executes a parallel auction with prebid **/
function executeParallelAuctionAlongsidePrebid(e, i, t) {
var FAILSAFE_TIMEOUT = 2000;
var requestManager = {
adserverRequestSent: false,
aps: false,
prebid: false
};
var slot,requestSlot;
var o = e.attr("id");
googletag.cmd.push(function () {
slot = googletag.defineSlot(i, t, o).addService(googletag.pubads()).setCollapseEmptyDiv(true, true);
});
googletag.cmd.push(function () {
requestSlot = googletag.pubads().getSlots().filter(item=>item.getSlotElementId() == o);
});
// when both APS and Prebid have returned, initiate ad request
function biddersBack() {
if (requestManager.aps && requestManager.prebid) {
sendAdserverRequest();
}
return;
}
// sends adserver request
function sendAdserverRequest() {
if (requestManager.adserverRequestSent === true) {
return;
}
requestManager.adserverRequestSent = true;
googletag.cmd.push(function() {
//googletag.display(o);
googletag.pubads().refresh([slot]);
});
}
// sends bid request to APS and Prebid
function requestHeaderBids() {
//Ignoring Brand Studio ads for header bidding
if(i.includes("_BS_")){
return sendAdserverRequest();
}
// APS request
apstag.fetchBids({
slots: [{
slotID: o,
slotName: i,
sizes: t
}]
},function(bids) {
googletag.cmd.push(function() {
apstag.setDisplayBids();
requestManager.aps = true; // signals that APS request has completed
biddersBack(); // checks whether both APS and Prebid have returned
});
}
);
// put prebid request here
PWT.requestBids(PWT.generateConfForGPT(requestSlot),function(adUnitArray){
PWT.addKeyValuePairsToGPTSlots(adUnitArray);
requestManager.prebid = true; // signals that Prebid request has completed
biddersBack(); // checks whether both APS and Prebid have returned
});
}
// initiate bid request
requestHeaderBids();
// set failsafe timeout
window.setTimeout(function() {
sendAdserverRequest();
}, FAILSAFE_TIMEOUT);
};
$.fn.isInViewport = function () {
if ($(this).length > 0) {
var e = $(this).offset().top - preViewportDistance,
i = e + $(this).outerHeight() + preViewportDistance,
t = $(window).scrollTop(),
o = t + $(window).height();
return i > t && e < o;
}
return !1;
};
var nextSlotId = 1;
function generateNextSlotName() {
var id = nextSlotId++;
return 'adslot' + id;
}
function storyAdDisplay(element){
element.each(function(){
var divid = generateNextSlotName();
$(this).attr('id', divid);
var divadcode = $(this).attr('data-adcode');
var divadsize = $(this).attr('data-adsize');
if (typeof divadcode !== "undefined" && typeof divadsize !== "undefined"){
divadsize = divadsize.replace(/'/g,'"');
var ad = $("#" + divid);
var homeScrollHandler = function () {
ad.isInViewport() && (executeParallelAuctionAlongsidePrebid(ad, divadcode, JSON.parse(divadsize)), $(window).off("scroll", homeScrollHandler));
};
$(window).on("scroll", homeScrollHandler);
}
});
}
var storyAds = [
{
adCode: "/1055314/HT_Tech_WAP_ES_In-Story_A_Multisize",
adSize: "[[200, 200], [250, 250], [300, 250], [336, 280]]"
},
{
adCode: "/1055314/HT_Tech_WAP_ES_In-Story_B_Multisize",
adSize: "['fluid', [200, 200], [250, 250], [300, 250], [336, 280]]"
},
{
adCode: "/1055314/HT_Tech_WAP_ES_In-Story_C_Multisize",
adSize: "['fluid', [200, 200], [250, 250], [300, 250], [336, 280]]"
},
{
adCode: "/1055314/HT_Tech_WAP_ES_In-Story_D_Multisize",
adSize: "['fluid', [200, 200], [250, 250], [300, 250], [336, 280]]"
},
{
adCode: "/1055314/HT_Tech_WAP_ES_In-Story_E_Multisize",
adSize: "['fluid', [200, 200], [250, 250], [300, 250], [336, 280]]"
},
{
adCode: "/1055314/HT_Tech_WAP_ES_In-Story_F_Multisize",
adSize: "['fluid', [200, 200], [250, 250], [300, 250], [336, 280]]"
},
{
adCode: "/1055314/HT_Tech_WAP_ES_In-Story_G_Multisize",
adSize: "['fluid', [200, 200], [250, 250], [300, 250], [336, 280]]"
}
];
var storyAdsDesk = [
{
adCode: "/1055314/HT_TECH_Desk_In-Story_A_Multisize",
adSize: "[[1, 1], [728, 90]]"
},
{
adCode: "/1055314/HT_TECH_Desk_In-Story_B_Multisize",
adSize: "[[1, 1], [728, 90]]"
},
{
adCode: "/1055314/HT_TECH_Desk_In-Story_C_Multisize",
adSize: "[[1, 1], [728, 90]]"
},
{
adCode: "/1055314/HT_TECH_Desk_In-Story_D_Multisize",
adSize: "[[1, 1], [728, 90]]"
},
{
adCode: "/1055314/HT_TECH_Desk_In-Story_E_Multisize",
adSize: "[[1, 1], [728, 90]]"
},
{
adCode: "/1055314/HT_TECH_Desk_In-Story_F_Multisize",
adSize: "[[1, 1], [728, 90]]"
},
{
adCode: "/1055314/HT_TECH_Desk_In-Story_G_Multisize",
adSize: "[[1, 1], [728, 90]]"
},
{
adCode: "/1055314/HT_TECH_Desk_In-Story_H_Multisize",
adSize: "[[1, 1], [728, 90]]"
}
];
function getNativeStoryAds(divMainArea, callback) {
try {
var thisStoryadCounter = 0;
var wordCount = 0;
var storyElemC = divMainArea.children("p,div.respVideo,figure");
var firstAdjuscentAdCounter = 0;
storyElemC.each(function (index, el) {
if (el.tagName.toLowerCase() == "p" || el.tagName.toLowerCase() == "figure" || (el.tagName.toLowerCase() == "div" && el.classList.contains("respVideo"))) {
if (!firstAdjuscentAdCounter) {
wordCount += analyseObjectData(el);
if (wordCount >= 30) {
firstAdjuscentAdCounter++;
wordCount = 0;
}
if (firstAdjuscentAdCounter) {
var newElem = document.createElement("div");
if(isDeviceEnabled('large')) {
newElem.setAttribute("data-adcode", storyAdsDesk[thisStoryadCounter].adCode);
newElem.setAttribute("data-adsize", storyAdsDesk[thisStoryadCounter].adSize);
newElem.classList.add("textC", "mb20", "ht-dfp-ad-large");
}
if(isDeviceEnabled('small')) {
newElem.setAttribute("data-adcode", storyAds[thisStoryadCounter].adCode);
newElem.setAttribute("data-adsize", storyAds[thisStoryadCounter].adSize);
newElem.classList.add("textC", "mb20", "ht-dfp-ad-small");
}
insertAfter(el, newElem);
thisStoryadCounter++;
}
} else {
wordCount += analyseObjectData(el);
if (wordCount >= 120) {
var newElem = document.createElement("div");
if(isDeviceEnabled('large')) {
newElem.setAttribute("data-adcode", storyAdsDesk[thisStoryadCounter].adCode);
newElem.setAttribute("data-adsize", storyAdsDesk[thisStoryadCounter].adSize);
newElem.classList.add("textC", "mb20", "ht-dfp-ad-large");
}
if(isDeviceEnabled('small')) {
newElem.setAttribute("data-adcode", storyAds[thisStoryadCounter].adCode);
newElem.setAttribute("data-adsize", storyAds[thisStoryadCounter].adSize);
newElem.classList.add("textC", "mb20", "ht-dfp-ad-small");
}
insertAfter(el, newElem);
wordCount = 0;
thisStoryadCounter++;
}
}
}
});
callback();
} catch (e) {
callback();
}
}
function analyseObjectData(obj) {
var embedWeight = 80;
if (typeof obj === "object")
if (obj.tagName.toLowerCase() == "p") {
var contentWords = (obj.innerText.match(/ /g) || []).length + 1;
return contentWords;
} else if (obj.tagName.toLowerCase() == "div" && obj.className.includes("respVideo")) return embedWeight;
else if (obj.tagName.toLowerCase() == "figure") return embedWeight;
return 0;
}
function insertAfter(element, newElement) {
element.parentNode.insertBefore(newElement, element.nextSibling);
}
var divMainArea = $(".htStoryDetail");
getNativeStoryAds(divMainArea, function(){
if(isDeviceEnabled('large')) {
storyAdDisplay($(".ht-dfp-ad-large"));
}
if(isDeviceEnabled('small')) {
storyAdDisplay($(".ht-dfp-ad-small"));
}
});
