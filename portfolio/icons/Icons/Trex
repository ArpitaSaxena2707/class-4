var reqOpen = true;
$(document).bind("scroll", function() {
var a = {
top: $(document).scrollTop(),
left: $(document).scrollLeft()
};
a.bottom = a.top + $(document).height();
lastScrollTop = a.top;
var c = a.bottom - $(document).height();
if (a.top > 200) {
$(".infiniteStoryScroll").addClass('show');
$("#stickydivAD").addClass('showAds');
$("#back-to-top").addClass('showTopBottom');
$(".respVideo").find('twitter-widget.twitter-tweet').each(function() {
$(this).closest(".respVideo").addClass("twitterClass");
});
$(".respVideo").find('.affiliation').each(function() {
$(this).closest(".respVideo").addClass("affiliationClass");
});
$(".respVideo").find('script').each(function() {
$(this).closest(".respVideo").addClass("scriptClass");
});
$(".respVideo").find('.textarea').each(function() {
$(this).closest(".respVideo").addClass("respShow");
});
$(".respVideo").find('.instagram-media').each(function() {
$(this).closest(".respVideo").removeClass("scriptClass");
$(this).closest(".respVideo").removeClass("respShow");
$(this).closest(".respVideo").addClass("instaClass");
$(this).closest(".instaClass").removeClass("respVideo");
});
if (reqOpen == true) {
$("#ulInfinite li:first").addClass("active");
$("#ulInfinite li .stroy-link").each(function(e, d) {
if (isDeviceEnabled('small')) {
if (e > 0 && e < 2) {
listUrl.push($(d).html())
}
} else {
if (e > 0) {
listUrl.push($(d).html())
}
}
});
InfiniteScroll();
reqOpen = false
}
} else {
if (c < 200) {
$(".infiniteStoryScroll").removeClass('show');
$("#stickydivAD").removeClass('showAds');
$("#back-to-top").removeClass('showTopBottom');
}
}
});
function getStoryIdByUrl(c) {
var a = "";
if (c !== "") {
var bd = c.substring(0, c.lastIndexOf("-"));
var b = c.match(bd + "-(.*).html");
a = b[1].trim()
}
return a
};
function getLatestNews(j) {
var str = "";
var k, e;
var forYouApiResponse = "";
if (j !== "") {
e = $(j).find("#latest-stories")
} else {
e = "#latest-stories"
}
if (forYouApiResponse == "") {
$.ajax({
url: "https://tech.hindustantimes.com/api/cms/page?url=/latest-news&page=0&size=6",
dataType: 'application/json',
success: function(data) {
GenerateData(data, e);
},
error: function(data) {
GenerateData(data, e);
}
});
}
}
function GenerateData(data, e) {
var str = '';
str += '<div class="widget widget-popular-posts" >';
str += '<h4 class="widget-title"><a href="/latest-news">Latest News</a></h4>';
str += '<ul class="widget-popular-posts__list">';
data = JSON.parse(data.responseText).content.content;
localStorage.setItem('obj' + e, data);
$.each(
data,
function(i, item) {
if (i < 4) {
str += '<li>';
str += '<article class="clearfix">';
str += '<div class="widget-popular-posts__img-holder">';
str += '<div class="thumb-container">';
str += '<a href="' + item.metadata.url + '">';
if (item.leadMedia.image && item.leadMedia.elements && item.leadMedia.image.images["90x90"]) {
str += '<img data-src="' + item.leadMedia.image.images["90x90"] + '" src="/img/zero.gif" class="lazyload">';
} else {
str += '<img data-src="/img/default/default_90_90_v1.png" src="/img/zero.gif" alt="Hindustan Time Tech" class="lazyload">';
}
str += '</a>';
str += '</div>';
str += '</div>';
str += '<div class="widget-popular-posts__entry">';
str += '<h3 class="widget-popular-posts__entry-title">';
str += '<a href="' + item.metadata.url + '">' + item.mobileHeadline + '</a>';
str += '</h3>';
str += '</div>';
str += '</article>';
str += '</li>';
}
});
str += '</ul>';
str += '</div>';
forYouApiResponse = str;
$(e).html(str)
}
</script>
