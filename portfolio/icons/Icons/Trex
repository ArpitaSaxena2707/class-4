<script>
function basicSearch(e, inputId) {
var searchText = document.getElementById(inputId).value;
var regex = /^[a-zA-Z0-9-,.@&;:’'"$%+ ]+$/;
if (regex.test(document.getElementById(inputId).value) == false || document.getElementById(inputId).value.trim() == "") {
var inputVal = document.getElementById(inputId).value;
document.getElementById(inputId).value = inputVal.substr(0, inputVal.length - 1)
}
if (e != "1") {
if (e.keyCode == 13) {
var tb = document.getElementById(inputId);
if (tb.value == "" || tb.value == null) {
alert("Please enter the keyword");
tb.focus();
return false
} else {
console.log(encodeURIComponent(tb.value));
top.location.href = "/search?text=" + encodeURIComponent(tb.value.replace("%", ""));
return false
}
}
} else {
var tb = document.getElementById(inputId);
if (tb.value == "" || tb.value == null || tb.value == "Search") {
alert("Please enter the keyword");
tb.focus();
return false
} else {
console.log(encodeURIComponent(tb.value));
location.href = "/search?text=" + encodeURIComponent(tb.value.replace("%", ""))
}
}
}
