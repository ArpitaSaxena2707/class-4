function comscoreContextual() {
var xhr = new XMLHttpRequest();
xhr.open('GET', 'https://segment-data.zqtk.net/htmedia-cna806f?url=' + encodeURIComponent("https://tech.hindustantimes.com" + window.location.pathname), true);
