<script>
$(document).ready(function(){
if(isMobile){
$(".colapse-text h3").on("click", function(){
$(this).addClass("plus");
$(this).toggleClass("close-btn");
$(this).next().toggle();
});
}
$(document).on("click", ".btn-more", function(e){
var btnId = $(e.currentTarget).attr("id");
var elemId = $(e.currentTarget).attr("data-article");
var collapseFlag = 0;
_viewMore(elemId, btnId, collapseFlag);
if(storyType === "liveblog"){
ga('send','event','liveblog_page', 'click','read_more');
}else{
ga('send','event','story_page', 'click','read_more');
}
})
});
function sendTopicGA(topicPath, topicName){
ga('send', 'event', 'Topics', topicName, topicPath);
}
function sharePage(url) {
newwindow = window.open(url, "name", "height=350,width=400,scrollbars=yes");
if (window.focus) {
newwindow.focus();
}
return false;
}
function _viewMore(articleId, btnId, flag){
console.log(btnId);
var elem = document.getElementById(articleId);
var btnEl = document.getElementById("btn-container-"+btnId.split("-")[1]);
if(flag === 0){
elem.style.height = 'auto';
btnEl.style.display = 'none';
}else{
elem.style.height = '240px';
btnEl.style.display = 'flex';
}
}
</script>
